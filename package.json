{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "storybook": "storybook start -p 7007",
    "clean-cache": "watchman watch-del-all && rm -rf node_modules && npm install && npm start -- --reset-cache"
  },
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.8.3",
